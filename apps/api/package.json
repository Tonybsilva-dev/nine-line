{
  "name": "@nine-line/api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format esm --outDir dist --splitting --shims --clean --no-dts --external @sentry/node",
    "dev": "tsx --watch src/index.ts",
    "start": "node --enable-source-maps dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:rbac": "tsx scripts/setup-rbac.sh",
    "prisma:studio": "prisma studio",
    "lint": "eslint src --fix --max-warnings 0",
    "check-types": "tsc --noEmit",
    "setup": "./scripts/setup-app.sh"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@sentry/node": "^10.0.0",
    "@types/handlebars": "^4.0.40",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.7",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.0.1",
    "git-commit-msg-linter": "^5.0.8",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailtrap": "^4.2.0",
    "nodemailer": "^7.0.5",
    "pino": "^8.17.2",
    "pino-http": "^10.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nine-line/eslint": "*",
    "@nine-line/tsconfig": "*",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.31.0",
    "fast-glob": "^3.3.3",
    "pino-pretty": "^13.0.0",
    "prisma": "^5.10.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.7.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
